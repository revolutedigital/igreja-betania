# Configuração do Banco de Dados (Railway PostgreSQL)
# Copie a URL do PostgreSQL no Railway Dashboard
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=public"

# NextAuth Configuration
# Gere um secret com: openssl rand -base64 32
NEXTAUTH_SECRET="sua-chave-secreta-aqui"
NEXTAUTH_URL="https://seu-app.railway.app"

# Código para criar administrador
ADMIN_CODE="SEU_CODIGO_ADMIN"

# VAPID Keys para Push Notifications (opcional)
# Gere com: npx web-push generate-vapid-keys
NEXT_PUBLIC_VAPID_PUBLIC_KEY=""
VAPID_PRIVATE_KEY=""
